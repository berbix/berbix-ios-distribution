/documentation\/BerbixToolkit\/CameraSession\/Mode\/document","kind":"symbol","type":"topic","url":"\/documentation\/berbixtoolkit\/camerasession\/mode\/document"},"doc://com.berbix.toolkit.BerbixToolkit/documentation/BerbixToolkit/CameraSession/Mode/selfie":{"role":"symbol","title":"CameraSession.Mode.selfie","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"selfie"}],"abstract":[{"type":"text","text":"The toolkit will optimize for front-facing selfie videos"}],"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraSession\/Mode\/selfie","kind":"symbol","type":"topic","url":"\/documentation\/berbixtoolkit\/camerasession\/mode\/selfie"},"doc://com.berbix.toolkit.BerbixToolkit/documentation/BerbixToolkit/CameraSession/Mode/application":{"role":"symbol","title":"CameraSession.Mode.application","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"application"}],"abstract":[{"type":"text","text":"Application only, the toolkit will not process any camera frames"}],"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraSession\/Mode\/application","kind":"symbol","type":"topic","url":"\/documentation\/berbixtoolkit\/camerasession\/mode\/application"},"doc://com.berbix.toolkit.BerbixToolkit/documentation/BerbixToolkit/CameraSession/onPhotoTaken":{"role":"symbol","title":"onPhotoTaken","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"onPhotoTaken"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"PhotoTakenBlock","preciseIdentifier":"s:13BerbixToolkit15PhotoTakenBlocka"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"Respond to a call to "},{"type":"reference","isActive":true,"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraSession\/capturePhoto()"}],"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraSession\/onPhotoTaken","kind":"symbol","type":"topic","url":"\/documentation\/berbixtoolkit\/camerasession\/onphototaken"},"doc://com.berbix.toolkit.BerbixToolkit/documentation/BerbixToolkit/CameraPreview":{"role":"symbol","title":"CameraPreview","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"CameraPreview"}],"abstract":[{"type":"text","text":"Real time preview of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraSession"},{"type":"text","text":" without any I\/O overhead"}],"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraPreview","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"CameraPreview"}],"url":"\/documentation\/berbixtoolkit\/camerapreview"},"doc://com.berbix.toolkit.BerbixToolkit/documentation/BerbixToolkit/CameraSession/stop()":{"role":"symbol","title":"stop()","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"stop"},{"kind":"text","text":"()"}],"abstract":[{"type":"text","text":"This will stop a running session."}],"identifier":"doc:\/\/com.berbix.toolkit.BerbixToolkit\/documentation\/BerbixToolkit\/CameraSession\/stop()","kind":"symbol","type":"topic","url":"\/documentation\/berbixtoolkit\/camerasession\/stop()"},"doc://com.berbix.toolkit.BerbixToolkit/documentation/BerbixToolkit/CameraSession/onFaceDetected":{"role":"symbol","title":"onFaceDetected","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"onFaceDetected"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"FaceDetectedBlock","preciseIdentifier":"s:13BerbixToolkit17FaceDetectedBlocka"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"Closure to handle a face detected in the frame"}],"identifier":"doc:\/\/c